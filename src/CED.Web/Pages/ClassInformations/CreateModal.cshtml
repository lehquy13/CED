@page
@using CED.Localization
@model CED.Web.Pages.ClassInformations.CreateModalModel
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@using Microsoft.Extensions.Localization
@section scripts
    {
    <abp-script src="/Pages/ClassInformations/Index.js" />
}
@inject IStringLocalizer<CEDResource> L

<abp-dynamic-form abp-model="ClassInformation" asp-page="/ClassInformations/CreateModal" onsubmit="return CreateUpdateClassInformationFormSubmited(this);">
    <abp-card>
        <abp-card-header title="@L["NewClass"].Value"></abp-card-header>
        <abp-card-body>
            <abp-form-content />
        </abp-card-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-card>
</abp-dynamic-form>